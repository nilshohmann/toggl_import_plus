    "keytar": "^4.0.5",
    "node-persist": "^2.1.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.0",
    "tslint": "^5.7.0",
    "typedi": "^0.5.2",
    "typescript": "^2.5.3"
  },
  "dependencies": {}
